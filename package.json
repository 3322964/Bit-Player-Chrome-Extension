{
  "name": "bit_player",
  "version": "1.0.0",
  "description": "video torrents player with subtitles support",
  "main": "bundle.js",
  "scripts": {
    "start": "node server.js",
    "browserify": "browserify -r browserify-fs:fs -r chrome-dgram:dgram -r chrome-socket:socket bundle.js -o ext/js/bundle.js",
    "bundle2": "browserify bundle2.js -o ext/js/bundle2.js",
    "my_bundle": "browserify -r browserify-fs:fs -r chrome-dgram:dgram -r chrome-net:net -r ./dns:dns my_bundle.js -o ext/js/my_bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roeycohen/Bit-Player-Chrome-Extension.git"
  },
  "keywords": [
    "pocorn",
    "player",
    "chrome",
    "extension"
  ],
  "author": "Roey Cohen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/roeycohen/Bit-Player-Chrome-Extension/issues"
  },
  "homepage": "https://github.com/roeycohen/Bit-Player-Chrome-Extension#readme",
  "dependencies": {
    "browser-process-hrtime": "0.1.2",
    "browserify-fs": "1.0.0",
    "buffer": "4.5.0",
    "chrome-dgram": "2.1.7",
    "chrome-net": "^3.1.1",
    "chrome-socket": "0.0.1",
    "filereader-stream": "^1.0.0",
    "jschardet": "^1.4.1",
    "long": "3.0.3",
    "memory-chunk-store": "^1.2.0",
    "opensubtitles": "0.0.3",
    "path": "0.12.7",
    "stream": "0.0.2",
    "torrent-memory-storage": "1.0.0",
    "torrent-stream": "1.0.2"
  },
  "devDependencies": {
    "browserify": "*",
    "watchify": "*"
  }
}
